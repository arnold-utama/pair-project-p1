<div
  class="fixed top-0 w-full bg-gray-900 border-b border-gray-700 py-3 px-5 flex justify-between items-center shadow-sm">
  <img src="https://i.pinimg.com/474x/af/15/7e/af157eba3d2b2dcf7a5d10dc71858c4e.jpg"
    style="border-radius: 20px; width: max-content;" alt="Instagram Logo" class="h-10" />
  <div class="fa-solid">Instagaram</div>
  <div style="display: flex">
    <% if (!session.user) { %>
      <a href="/login">
        <div class="flex items-center gap-3 cursor-pointer hover:text-gray-400 fa-solid fa-right-to-bracket"
          style="padding-right: 20px">
          Login
        </div>
      </a>
      <% } else { %>
        <a href="/logout">
          <div class="flex items-center gap-3 cursor-pointer hover:text-gray-400 fa-solid fa-right-from-bracket">
            Logout
          </div>
        </a>
        <% } %>
  </div>
</div>